		result = prime * result + ((performance == null) ? 0 : performance.hashCode());
		result = prime * result + ((portletName == null) ? 0 : portletName.hashCode());
		result = prime * result + ((reason == null) ? 0 : reason.hashCode());
		return result;
	}
	@Override
	public boolean equals(Object obj) {
		if (this == obj)
			return true;
		if (obj == null)
			return false;
		if (getClass() != obj.getClass())
			return false;
		DocumentDTO other = (DocumentDTO) obj;
		if (createDate == null) {
